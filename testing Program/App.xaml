<Application x:Class="testing_Program.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:testing_Program"
             xmlns:syncfusionskin ="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <FontFamily x:Key="Mulish">pack://application:,,,/Fonts/#Mulish</FontFamily>
        <FontFamily x:Key="Montserrat">pack://application:,,,/Fonts/#Montserrat</FontFamily>
    </Application.Resources>
</Application>
